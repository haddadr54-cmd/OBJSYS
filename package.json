{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "nodemon --watch server --ext js,mjs,cjs,ts server/index.js",
    "server:watch": "node --watch server/index.js",
    "web": "vite",
    "dev:full": "npm run dev",
    "dev:all": "concurrently -k -n server,web -c cyan,magenta \"npm:server\" \"npm:dev\"",
    "health": "node scripts/health-check.cjs",
    "provision:audit": "node scripts/provision-audit.cjs",
    "typecheck": "tsc --noEmit",
    "lint:fix": "eslint . --fix"
    ,"dev:node": "node scripts/dev-all.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "lucide-react": "^0.344.0",
    "qrcode-terminal": "^0.12.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "whatsapp-web.js": "^1.34.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.4",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
